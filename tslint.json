{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "object-literal-shorthand": [ true, "never"],
        "object-literal-sort-keys": false,
        "variable-name": { "options": [ "allow-leading-underscore" ]},
        "trailing-comma": false
    },
    "rulesDirectory": []
}